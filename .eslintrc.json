{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent"           : [1, 4],
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "import/extensions": ["error", "ignorePackages", {"js" : "always"}],
        "comma-dangle": ["error", "never"],
        "no-param-reassign": 0,
        "no-else-return": 0,
        "prefer-template": 0,
        "no-shadow": 0,
        "no-alert": 0,
        "prefer-promise-reject-errors": 0,
        "no-var": 0,
        "no-unused-vars": 0,
        "import/no-unresolved": 0,
        "no-constant-condition": 0,
        "no-console": 0,
        "consistent-return": 0,
        "no-restricted-globals": 0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "class-methods-use-this": 0,
        "no-new": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "no-undef": 0,
        "eqeqeq":0,
        "max-len":0,
        "no-use-before-define":0
    }
}
